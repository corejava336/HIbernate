<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <!-- Entity mapping definition goes here -->
    <class name="com.nt.entity.student_entity" table="student" lazy="true" proxy="com.nt.entity.istudent">
    <id name="rollno">
    <column name="ROLLNO"></column>
    </id>
    <property name="name">
    <column name="NAME"></column>
    </property>
    <property name="city">
    <column name="CITY"></column>
    </property>
    </class>
</hibernate-mapping>
